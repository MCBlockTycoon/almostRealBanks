package org.rominos2.RealBanks.api.Settings;

public abstract interface LanguageSettings {
	public abstract String getLang(SentenceType paramSentenceType,
			String paramString1, String paramString2,
			String[] paramArrayOfString);

	public static enum SentenceType {
		BANK, CLIENT, BANK_CLIENT, BANK_VALUE, BANK_CLIENT_VALUE, BANK_WORLD, LIST, BANK_LIST, VALUE, WORLD, NORMAL;
	}
}