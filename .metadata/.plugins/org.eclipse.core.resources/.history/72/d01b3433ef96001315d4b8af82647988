package org.rominos2.RealBanks.api.Settings;

import java.io.File;
import org.rominos2.RealBanks.api.Settings.IO.BankIO;

public abstract interface BankSettings {
	public abstract String getName();

	public abstract File getFile();

	public abstract BankIO getSave();

	public abstract int getAccountsLimit();

	public abstract void setAccountLimit(int paramInt);

	public abstract String getEnterPermission();

	public abstract void setEnterPermission(String paramString);

	public abstract boolean isLogTransactions();

	public abstract void setLogTransactions(boolean paramBoolean);

	public abstract double getEnterCost();

	public abstract void setEnterCost(double paramDouble);
}