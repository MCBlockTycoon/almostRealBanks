package org.rominos2.RealBanks.api.Banks;

import org.bukkit.OfflinePlayer;
import org.bukkit.entity.Player;
import org.bukkit.inventory.ItemStack;

public abstract interface BankAccount {
	public abstract OfflinePlayer getPlayer();

	public abstract Bank getBank();

	public abstract ItemStack[] getContentClone();

	public abstract boolean addItem(ItemStack paramItemStack,
			Player paramPlayer, String paramString);

	public abstract boolean removeItem(ItemStack paramItemStack,
			Player paramPlayer, String paramString);
}